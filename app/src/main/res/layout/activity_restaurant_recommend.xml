<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="#FFFFFF"
  android:orientation="vertical">

  <TextView
    android:id="@+id/textView7"
    android:textStyle="bold"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:background="#009688"
    android:gravity="fill_vertical"
    android:text="  식당 추천"
    android:textColor="#FFFFFF"
    android:textSize="30dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"/>
  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.1"/>
  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.50121653"/>
  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.02676399"/>
  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline10"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.98"/>

  <LinearLayout
    android:id="@+id/typeLinearLayout"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginEnd="8dp"
    android:padding="15dp"
    android:background="@drawable/border"
    android:orientation="vertical"
    app:layout_constraintEnd_toStartOf="@+id/guideline5"
    app:layout_constraintStart_toStartOf="@+id/guideline9"
    app:layout_constraintTop_toTopOf="@+id/guideline3">
    <CheckBox
      android:id="@+id/typeCheckBox1R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="한식"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/typeCheckBox2R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="일식"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/typeCheckBox3R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="중식"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/typeCheckBox4R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="양식"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/typeCheckBox5R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="기타"
      android:textColor="#000000"/>
  </LinearLayout>
  <TextView
    android:id="@+id/textView12"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_alignParentTop="true"
    android:background="#ffffff"
    android:gravity="left"
    android:text="   종류   "
    android:textSize="17sp"
    app:layout_constraintStart_toStartOf="@+id/guideline9"
    app:layout_constraintTop_toTopOf="@+id/guideline3"/>
  <LinearLayout
    android:id="@+id/numOfPeopleLinearLayout"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:padding="15dp"
    android:background="@drawable/border"
    android:orientation="vertical"
    app:layout_constraintEnd_toStartOf="@+id/guideline10"
    app:layout_constraintStart_toStartOf="@+id/guideline5"
    app:layout_constraintTop_toTopOf="@+id/guideline3">
    <CheckBox
      android:id="@+id/numOfPeopleCheckBox1R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="1명 가능"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/numOfPeopleCheckBox2R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="2명 가능"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/numOfPeopleCheckBox3R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="3명 가능"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/numOfPeopleCheckBox4R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="4명 가능"
      android:textColor="#000000"/>
    <CheckBox
      android:id="@+id/numOfPeopleCheckBox5R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="5명 이상 가능"
      android:textColor="#000000"/>
  </LinearLayout>
  <TextView
    android:id="@+id/textView8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_alignParentTop="true"
    android:background="#ffffff"
    android:gravity="left"
    android:text="   인원수   "
    android:textSize="17sp"
    app:layout_constraintStart_toStartOf="@+id/guideline5"
    app:layout_constraintTop_toTopOf="@+id/guideline3"/>
  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline11"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.44"/>
  <LinearLayout
    android:id="@+id/costLinearLayout"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:padding="15dp"
    android:background="@drawable/border"
    android:orientation="vertical"
    app:layout_constraintEnd_toStartOf="@+id/guideline5"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/guideline11">
    <CheckBox
      android:id="@+id/costCheckBox1R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="5000원 미만"
      android:textColor="#000000"
      android:textSize="11.2sp"/>
    <CheckBox
      android:id="@+id/costCheckBox2R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="5000원 이상 10000원 미만"
      android:textColor="#000000"
      android:textSize="11.2sp"/>
    <CheckBox
      android:id="@+id/costCheckBox3R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="10000원 이상 15000원 미만"
      android:textColor="#000000"
      android:textSize="10.8sp"/>
    <CheckBox
      android:id="@+id/costCheckBox4R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="15000원 이상 20000원 미만"
      android:textColor="#000000"
      android:textSize="10.8sp"/>
    <CheckBox
      android:id="@+id/costCheckBox5R"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="20000원 이상"
      android:textColor="#000000"
      android:textSize="11.2sp"/>
  </LinearLayout>
  <TextView
    android:id="@+id/textView14"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_alignParentTop="true"
    android:background="#ffffff"
    android:gravity="left"
    android:text="   가격   "
    android:textSize="17sp"
    app:layout_constraintStart_toStartOf="@+id/guideline9"
    app:layout_constraintTop_toTopOf="@+id/guideline11"/>
  <ScrollView
    android:id="@+id/scrollView2"
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    app:layout_constraintBottom_toTopOf="@+id/guideline12"
    app:layout_constraintEnd_toStartOf="@+id/guideline10"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="@+id/guideline5"
    app:layout_constraintTop_toTopOf="@+id/guideline11"
    app:layout_constraintVertical_bias="1.0">
    <LinearLayout
      android:id="@+id/locationLinearLayoutR"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:paddingTop="9dp"
      android:background="@drawable/border"
      android:orientation="vertical"/>
  </ScrollView>
  <androidx.constraintlayout.widget.Guideline
    android:id="@+id/guideline12"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintGuide_percent="0.78"/>
  <TextView
    android:id="@+id/textView18"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_alignParentTop="true"
    android:background="#ffffff"
    android:gravity="left"
    android:text="   장소   "
    android:textSize="17sp"
    app:layout_constraintStart_toStartOf="@+id/guideline5"
    app:layout_constraintTop_toTopOf="@+id/guideline11"/>
  <Button
    android:id="@+id/recommendButton"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp"
    android:text="식당 추천받기"
    app:layout_constraintBottom_toTopOf="@+id/recommendTextView"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"/>
  <TextView
    android:id="@+id/recommendTextView"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:layout_marginBottom="24dp"
    android:textSize="20sp"
    android:text="'식당 추천받기' 버튼을 누르세요"
    android:gravity="center"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>